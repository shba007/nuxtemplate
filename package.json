{
  "name": "nuxtemplate",
  "version": "0.6.0",
  "description": "Nuxt Template",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shba007/nuxtemplate.git"
  },
  "type": "module",
  "private": true,
  "volta": {
    "node": "22.11.0"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || ^22.0.0",
    "pnpm": "^9.12.0"
  },
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev --tunnel",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier .  --write",
    "commit": "oco",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "clean": "nuxt cleanup",
    "start": "node .output/server/index.mjs",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build --build-arg VERSION=dev --build-arg BUILD_TIME=now -t nuxtemplate:dev .",
    "docker:start": "docker run --detach --name nuxtemplate-dev --env-file .env.prod -p 4000:3000 nuxtemplate:dev",
    "tunnel": "untun tunnel --port 4000 http://localhost:4000",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:dev:mobile": "tauri android dev",
    "tauri:build:mobile": "tauri android build"
  },
  "dependencies": {
    "@nuxt/content": "^2.13.4",
    "@nuxt/eslint": "^0.7.2",
    "@nuxt/fonts": "^0.10.2",
    "@nuxt/icon": "^1.9.0",
    "@nuxt/image": "^1.8.1",
    "@nuxt/scripts": "^0.9.5",
    "@nuxt/test-utils": "^3.14.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^9.1.0",
    "@nuxtjs/seo": "2.0.2",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia/nuxt": "^0.8.0",
    "@prisma/nuxt": "^0.1.2",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/nuxt": "^12.0.0",
    "nuxt": "^3.14.1592",
    "nuxt-auth-utils": "^0.5.5",
    "pinia": "^2.2.8",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@dotenvx/dotenvx": "^1.26.0",
    "@prisma/client": "6.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@tauri-apps/cli": "^2.1.0",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "opencommit": "^3.2.2",
    "playwright-core": "^1.49.0",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "6.0.0",
    "semantic-release": "^24.2.0",
    "untun": "^0.1.3",
    "vitest": "^2.1.6"
  }
}