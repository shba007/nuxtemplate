{
  "name": "nuxtemplate",
  "version": "1.0.0",
  "description": "Nuxt Template",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/shba007/nuxtemplate.git"
  },
  "type": "module",
  "private": true,
  "volta": {
    "node": "22.14.0"
  },
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0"
  },
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare && prisma generate",
    "dev": "nuxt dev --tunnel",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "clean": "nuxt cleanup",
    "start": "node .output/server/index.mjs",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build --build-arg VERSION=dev --build-arg BUILD_TIME=now -t nuxtemplate:dev .",
    "docker:start": "docker run --detach --name nuxtemplate-dev --env-file .env.prod -p 4000:3000 nuxtemplate:dev",
    "tunnel": "untun tunnel --port 4000 http://localhost:4000",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:dev:mobile": "tauri android dev",
    "tauri:build:mobile": "tauri android build"
  },
  "prisma": {
    "seed": "jiti prisma/seed.ts"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.1.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/icon": "^1.10.3",
    "@nuxt/image": "^1.9.0",
    "@nuxt/scripts": "^0.10.3",
    "@nuxt/test-utils": "^3.15.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^9.2.0",
    "@nuxtjs/seo": "2.1.1",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@pinia/nuxt": "^0.10.1",
    "@prisma/client": "^6.4.0",
    "@prisma/nuxt": "0.3.0",
    "@tauri-apps/api": "^2.2.0",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/core": "^12.7.0",
    "@vueuse/nuxt": "^12.7.0",
    "nuxt": "^3.15.4",
    "nuxt-auth-utils": "^0.5.15",
    "pinia": "^3.0.1",
    "prisma": "^6.4.0",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@tauri-apps/cli": "^2.2.7",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.20.1",
    "happy-dom": "^17.1.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "playwright-core": "^1.50.1",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "semantic-release": "^24.2.3",
    "typescript": "^5.7.3",
    "untun": "^0.1.3",
    "vitest": "^3.0.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "better-sqlite3",
      "esbuild",
      "prisma",
      "sharp"
    ]
  }
}